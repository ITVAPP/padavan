CONFIG_FIRMWARE_INCLUDE_HTOP=n

# 包含 NANO 文本编辑器
CONFIG_FIRMWARE_INCLUDE_NANO=n

# 包含 IPERF3 网络性能测试工具
CONFIG_FIRMWARE_INCLUDE_IPERF3=n

